{
  "name": "linked-role-lib",
  "description": "Discord Linked Role API Library",
  "version": "0.0.0",
  "module": "build/index.js",
  "type": "module",
  "private": false,
  "packageManager": "bun@1.0.31",
  "author": {
    "name": "m1sk9",
    "email": "me@m1sk9.dev",
    "url": "https://m1sk9.dev"
  },
  "scripts": {
    "build": "bun build --entrypoints ./src/index.ts --outdir ./build --target bun",
    "format": "bunx @biomejs/biome format --write ./src",
    "lint": "bunx @biomejs/biome lint ./src",
    "check": "bunx @biomejs/biome check ./src",
    "check:apply": "bunx @biomejs/biome check --apply ./src",
    "check:type": "tsc --noEmit",
    "postinstall": "lefthook install"
  },
  "dependencies": {
    "@mikuroxina/mini-fn": "^5.6.0",
    "dotenv": "^16.4.5",
    "lefthook": "^1.6.7"
  },
  "devDependencies": {
    "@biomejs/biome": "^1.6.1",
    "@types/bun": "^1.0.8"
  },
  "peerDependencies": {
    "typescript": "^5.0.0"
  }
}
